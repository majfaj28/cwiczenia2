using System;

public class HelloWorld
{
    public static void Main(string[] args)
    {
      int i = 1;
      while (i<=50)
        {
            if (i % 2 ==0)
            {
                Console.WriteLine(i + " - parzysta");
            }
            else
            {
                Console.WriteLine(i + " - nieparzysta");
            }
            i++;
        }
    }
}
