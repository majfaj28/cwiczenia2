using System;

public class HelloWorld
{
    public static void Main(string[] args)
    {
     Console.WriteLine("Podaj wybrana liczbe n");
     int n = int.Parse(Console.ReadLine());
     
     for (int i=2; i< n; i++)
     {
         bool pierwsza = true;
         for(int d=2; d<i; d++)
         {
             if(i%d==0)
             {
                 pierwsza = false;
                 break;
             }
         }
         if (pierwsza == true)
         {
             Console.WriteLine(i);
         }
     }
    }
}
