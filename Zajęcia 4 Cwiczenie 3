using System;
using System.Collections.Generic;
public class HelloWorld
{
    public static void Main(string[] args)
    {
        List<int> numbers = new List<int>{2,4,5,6,7};
        int sum=0;
        foreach (int n in numbers)
        {
            sum += n;
        }
        Console.WriteLine("Suma:" + sum);
    }
}
